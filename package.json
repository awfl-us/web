{
  "name": "@awfl/web",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "tsup",
    "typecheck": "tsc -b",
    "lint": "eslint .",
    "check": "npm run typecheck && npm run lint",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepack": "npm run build:lib",
    "allow:firewall": "bash scripts/allow-macos-firewall.sh"
  },
  "dependencies": {
    "firebase": "^12.2.1"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "esbuild-css-modules-plugin": "^3.1.5",
    "esbuild-plugin-css-modules": "^0.3.0",
    "esbuild-plugin-postcss2": "^0.1.2",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "postcss-modules": "^6.0.1",
    "tsup": "^8.5.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^2.1.9"
  },
  "exports": {
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
